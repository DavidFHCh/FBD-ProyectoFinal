  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>semovi</DataSourceName>
        <CommandText>declare @s2 table (
	idcamara int,
	fotomultas int
);
insert into @s2 (idcamara, fotomultas)
select idcamara,
	count(*)
from fotomulta
where idcamara is not null
group by idcamara;

declare @r int;
select @r = max(fotomultas)
from @s2;

select *
from camara
where idcamara in (
	select idcamara
	from @s2
	where fotomultas = @r
);</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
