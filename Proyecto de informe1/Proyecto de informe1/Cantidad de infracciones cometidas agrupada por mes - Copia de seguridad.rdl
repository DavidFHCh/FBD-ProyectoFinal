        </Field>
      </Fields>
      <Query>
        <DataSourceName>SEMOVICDMX</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT DATEPART(month, fecha) Mes, COUNT(numExpediente) Multas
FROM
	((SELECT fecha, numExpediente FROM MultaAgente) UNION
	(SELECT fecha, numExpediente FROM FotoMulta)) AS s
GROUP BY DATEPART(month, fecha);</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
